default partial alphanumeric_keys
xkb_symbols "basic" {
        include "ch"
        name[Group1] = "Modified programmer keyboard";
        key <AC03> { [ d, D, braceleft ] };
        key <AC04> { [ f, F, braceright ] };

        key <AD03> { [ e, E, parenleft ] };
        key <AD04> { [ r, R, parenright ] };
        
        key <AB03> { [ c, C, bracketleft ] };
        key <AB04> { [ v, V, bracketright ] };

        key <AD08> { [ i, I, bar] };
};
